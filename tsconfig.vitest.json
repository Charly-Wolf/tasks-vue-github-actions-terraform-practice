{
  "extends": "./tsconfig.app.json",
  "include": [
    "env.d.ts",
    "tests/**/*",
    "tests/**/*.vue",
    "tests/**/*.ts",
    "tests/**/*.json",
    "src/**/*",
    "src/**/*.vue",
    "src/**/*.ts",
    "src/**/*.json"
  ],
  "exclude": [],
  "compilerOptions": {
    "composite": true,
    "preserveValueImports": false,
    "importsNotUsedAsValues": "remove",
    "verbatimModuleSyntax": true,
    "lib": [],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.vitest.tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["vitest/globals", "node", "jsdom"]
  }
}
